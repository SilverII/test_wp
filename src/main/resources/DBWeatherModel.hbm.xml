<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="weather.project.model.DBWeatherModel" table="dataw">
        <meta attribute="class-description">
            This class contains developer's details.
        </meta>
        <id name="id" type="int" column="ID">
            <generator class="native"/>
        </id>
        <property name="city" column="CITY" type="string"/>
        <property name="src" column="SRC" type="string"/>
        <property name="temp" column="TEMP" type="string"/>
        <property name="wind" column="WIND" type="string"/>
        <property name="cloud" column="CLOUD" type="string"/>
        <property name="api" column="API" type="string"/>
    </class>
</hibernate-mapping>